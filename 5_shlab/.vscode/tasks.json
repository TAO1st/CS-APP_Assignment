{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "build tsh",
			"command": "/usr/bin/gcc",
			"args": [
				"-Wall",
				"-O2",
				"-g",
				"tsh.c",
				"-o",
				"${fileDirname}/tsh"
			],
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}